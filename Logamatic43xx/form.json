{	
	"elements":
        [
            {"type": "NumberSpinner", "name": "Bus", "caption": "ECO-CAN Adresse:"}
        ],
    "actions":
        [
            {
                "type": "Button",
                "label": "Logamatic Auslesen und Module anlegen",
                "onClick": "Logamatic_RequestModule($id);"
            },
            {
                "type": "Button",
                "label": "Monitordaten auslesen",
                "onClick": "Logamatic_RequestMonitordaten($id);"
            },
            {
                "type": "Button",
                "label": "einstellbare Parameter auslesen",
                "onClick": "Logamatic_RequestEinstellPar($id);"
            }
        ],
    "status":
        [
            {
                "code": 102,
                "icon": "active",
                "caption": "Konfiguration gültig."
            },
            {
                "code": 202,
                "icon": "error",
                "caption": "ECO-CAN Adresse ungültig"
            }
        ]
}